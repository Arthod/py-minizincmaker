include "alldifferent.mzn";
var 1..8: q_1;
var 1..8: q_2;
var 1..8: q_3;
var 1..8: q_4;
var 1..8: q_5;
var 1..8: q_6;
var 1..8: q_7;
var 1..8: q_8;
constraint alldifferent([q_1, q_2, q_3, q_4, q_5, q_6, q_7, q_8]);
constraint alldifferent([q_1 + 1, q_2 + 2, q_3 + 3, q_4 + 4, q_5 + 5, q_6 + 6, q_7 + 7, q_8 + 8]);
constraint alldifferent([q_1 - 1, q_2 - 2, q_3 - 3, q_4 - 4, q_5 - 5, q_6 - 6, q_7 - 7, q_8 - 8]);
solve satisfy;
